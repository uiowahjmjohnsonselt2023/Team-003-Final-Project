<h1>Create New To-Do Item</h1>

<%= form_with model: @listing, url: listings_path, method: :post, local: true do |f| %>

  <%= f.text_field :title %>
  <%= f.text_area :description %>
  <%= f.text_field :price %>

  <%= f.text_field :condition %>
  <%= f.text_field :quantity %>
  <%= f.text_field :images %>
  <%= f.submit %>

<% end %>