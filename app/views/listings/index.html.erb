<h1>Listings</h1>

<table>
  <thead>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @listings.each do |listing| %>
    <tr>
      <td><%=link_to listing.title, listing %></td>
      <td><%= listing.description %></td>
    </tr>
  <% end %>
  </tbody>
</table>


